{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js",
      "methods": ["GET", "POST", "PATCH", "DELETE"],
      "headers": {
        "Access-Control-Allow-Origin": "https://notes-app-frontend-gamma.vercel.app, http://localhost:3000",
        "Access-Control-Allow-Methods": "GET, POST, PATCH, DELETE",
        "Access-Control-Allow-Credentials": "true"
      }
    }
  ]
}
